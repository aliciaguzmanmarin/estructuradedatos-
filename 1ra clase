#include "stdafx.h"
#include "stdlib.h"
#include "stdio.h"
#include "iostream"
#include "conio.h"
 using namespace std ;



void main ()//prototipos. encabezado de un subprograma //
{   float  P,p ;
    do {cout<<"ingrese precio : ";
        cin>> P ;}
    while (P<0);

    p=(P*0.13)+P;//acumulador//

    cout<<endl<<"Precio + IVA : "<< p ;
    getch();
}
